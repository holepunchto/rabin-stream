{
  "name": "rabin-stream",
  "version": "2.0.0-0",
  "description": "Streaming Rabin chunker",
  "exports": {
    "./package": "./package.json",
    ".": "./index.js"
  },
  "imports": {
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    }
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "prettier --check . && lunte",
    "test": "npm run test:node && npm run test:bare",
    "test:bare": "brittle-bare --coverage test.js",
    "test:node": "brittle-node --coverage test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/rabin-stream.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/rabin-stream/issues"
  },
  "homepage": "https://github.com/holepunchto/rabin-stream#readme",
  "dependencies": {
    "rabin-native": "^2.0.0-6",
    "streamx": "^2.23.0"
  },
  "devDependencies": {
    "bare-fs": "^4.5.4",
    "bare-path": "^3.0.0",
    "brittle": "^3.1.0",
    "lunte": "^1.3.0",
    "prettier": "^3.6.2",
    "prettier-config-holepunch": "^2.0.0"
  }
}
